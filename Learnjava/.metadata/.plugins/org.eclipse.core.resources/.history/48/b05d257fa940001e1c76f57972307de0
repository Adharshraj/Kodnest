package PrimeNumbersPrinter;

import java.util.Scanner;

public class Main {
	public static void printPrimes(int n)
	{
	int	count = 0;
	for (int i=1;i<=n;i++) {
		for(int j=1;j<=n;j++) {
			if(i%j==0) {
				count++;
			if(count==2) {
				System.out.print(i);
			}
		}
	}
		
	}
	}
	public static void main(String[] args) {
		Scanner scan =new Scanner(System.in);
		System.out.println("Enter a numer");
		int n = scan.nextInt();
		printPrimes(n); } 


}
