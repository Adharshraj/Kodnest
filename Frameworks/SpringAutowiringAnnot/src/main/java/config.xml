<?xml version="1.0" encoding="UTF-8"?>
 <beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xmlns:context="http://www.springframework.org/schema/context"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 <context:annotation-config/>
 <bean class="com.learn.Spring.SpringAutowiringAnnot.Professor" id="Pr1">
 	<property name="prId" value="13"></property>
 	<property name="prName" value="Amit"></property>
 	<property name="prSub" value="Maths"></property>
 </bean>
 <bean class="com.learn.Spring.SpringAutowiringAnnot.Professor" id="Pr2">
 	<property name="prId" value="14"></property>
 	<property name="prName" value="Subham"></property>
 	<property name="prSub" value="Physics"></property>
 </bean>
 <bean class="com.learn.Spring.SpringAutowiringAnnot.Professor" id="Pr3">
 	<property name="prId" value="15"></property>
 	<property name="prName" value="Abhishek"></property>
 	<property name="prSub" value="Chemistry"></property>
 </bean>
 <bean class="com.learn.Spring.SpringAutowiringAnnot.University" id="UNI"></bean>
 </beans>