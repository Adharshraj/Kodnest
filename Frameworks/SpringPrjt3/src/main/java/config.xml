<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
 <bean class="com.learn.Spring.SpringPrjt3.Employee" id="Emp">
 	<property name="Technology">
 		<list>
 			<value>Java</value>
 			<value>Python</value>
 			<value>SQL</value>
 		</list>
 	</property>
 	<property name="Project">
 		<set>
 			<value>Project-1</value>
 			<value>Project-2</value>
 			<value>Project-3</value>
 		</set>
 	</property>
 	<property name="Company">
 	<map>
 		<entry key="121" value="XYZ Ltd"></entry>
 		<entry key="456" value="ABCD Ltd"></entry>
 	</map>
 	</property>
 </bean>
 
 
 </beans>